# API Key de NASA 
# Obtener gratis en: https://api.nasa.gov/
# Completa el formulario y recibirás tu clave por email
NASA_API_KEY=DEMO_KEY
NASA_BASE_URL=https://api.nasa.gov

# API Key de OpenAI  
# Obtener en: https://platform.openai.com/account/api-keys
# Necesitas una cuenta de OpenAI y créditos disponibles
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Base de datos SQLite
# La base de datos se creará automáticamente si no existe
DATABASE_PATH=./mars_weather.db

# Puerto del servidor
PORT=3000

# Entorno de ejecución 
NODE_ENV=development

